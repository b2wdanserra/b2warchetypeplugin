{
  "name": "b2warchetype",
  "description": "bit2win archetype manager plugin for sfdx projects",
  "version": "1.0.16",
  "author": "Francesco Imperioli",
  "bugs": "https://github.com/fimperioli-b2w/b2warchetypeplugin/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.5",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2.28.0",
    "@sinonjs/fake-timers": "^8.0.1",
    "@types/sinonjs__fake-timers": "^6.0.4",
    "csvtojson": "^2.0.10",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "node-color-log": "^5.3.1",
    "path": "^0.12.7",
    "sfdx-node": "^3.1.0",
    "tslib": "^1",
    "typescript": "^4.4.0-beta"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/src/b2wlib/util_data",
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/fimperioli-b2w/b2warchetypeplugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fimperioli-b2w/b2warchetypeplugin",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
